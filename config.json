{
   "app":{
      "port":8000,
      "debug":true
   },
   "static-rules":[
      {
         "from":"/g",
         "to":"http://www.google.com",
         "landingpage":true
      },
      {
         "from":"/d",
         "to":"http://www.duckduckgo.com"
      }
   ],
   "replace-rules":[
      {
         "pattern":"/g/{query}",
         "target":"http://www.google.com?query={query}"
      }
   ],
   "database-rewriters":[
      {
         "path":"db/rules.sqlite3"
      }
   ]
}
